<!DOCTYPE html>
<html data-wf-page="{{ wfPage }}" data-wf-site="633ad4e42c7da0c6cfbbb8ac" lang="{{ request.locale.iso_code }}" data-money-format="{{ shop.money_format | escape }}">
  <head>{%- liquid
    if headerSnippetPath
        include headerSnippetPath
    else
        render "__default-head"
    endif
-%}
    {{content_for_header}}
    {% if template contains 'customers' %}
      {{ "shopify_common.js" | shopify_asset_url | script_tag }}
    {% endif %}
  </head>
  <body class="{{ bodyClasses }} {{ "template-" | append: request.page_type | handle }}">
    {% if headerSnippetPath %}
      {{ content_for_layout }}
    {% else %}
      {% include "backup-layout" %}
    {% endif %}

    {% render 'app_wishlist-king' %}
    <script src="{{ 'filter.js' | asset_url }}" type="text/javascript"></script>
  </body>
</html>