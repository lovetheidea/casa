.filter-form-mobile .close-mobile-filter,
.filter-form-mobile--trigger{
  display: none;
}

.filter-form-mobile .close-mobile-filter {
  position: absolute;
  top: 50px;
  left: 30px;
  width: 37px;
  height: 37px;
  align-items: center;
  background: none;
  padding: 0 6px;
}

.filter-form-mobile .close-mobile-filter svg{
  fill: #0b537f;
}

.filter-form-mobile--trigger{
  margin-bottom: 20px;
  border: 1px solid #0b537f;
  border-radius: 30px;
  align-items: center;
  padding: 10px 20px;
  font-family: Larken,sans-serif;
}

.filter-form-mobile--trigger svg{
  width: 20px;
  height: 20px;
  margin-right: 8px;
}

.filter-form-mobile--trigger svg,
.filter-form-mobile--trigger span{
  pointer-events: none;
}

@media screen and (max-width: 767px) {
  .filter-form-mobile--trigger,
  .filter-form-mobile .close-mobile-filter{
    display: block;
  }

  .filter-form-mobile {
    position: fixed;
    transform: translate(-100%);
    z-index: 9999;
    top: 0;
    left: 0;
    background: white;
    width: 100%;
    max-width: 100%;
    padding: 125px 40px 40px;
    height: 100%;
    transition: transform .2s;
  }

  .filter-form-mobile.is-open-filter {
    transform: translate(0%);
    transition: transform .2s;
  }

  .filter-form-mobile .filter-form{
    overflow: hidden;
  }

  .filter-form-mobile .f_item_list{
    display: flex;
    flex-wrap: wrap;
  }

  .filter-form-mobile .f_item_list .filter-group-display__list-item{
    width: 50%;
  }

  .product-list_products {
    max-width: 100%;
  }
}
